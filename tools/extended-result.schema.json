        },
        "Meta": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "uuid": {
                    "type": "string",
                    "format": "uuid"
                },
                "creation_date": {
                    "type": "string",
                    "format": "date-time"
                },
                "upload_date": {
                    "format": "date-time",
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ]
                }
            },
            "required": [
                "creation_date",
                "upload_date",
                "uuid"
            ],
            "title": "Meta"
        },
        "Result": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "benchmark_id": {
                    "type": "string"
                },
                "benchmark_name": {
                    "type": "string"
