                "formatted": {
                    "type": "string"
                }
            },
            "required": [
                "formatted",
                "normalized"
            ],
            "title": "Accuracy"
        },
        "Dataset": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "data_path": {
                    "type": "string"
