        },
        "BackendSettings": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "accelerator_code": {
                    "type": "string"
                },
                "accelerator_desc": {
                    "type": "string"
                },
                "framework": {
                    "type": "string"
