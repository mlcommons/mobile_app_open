                "batch_size": {
                    "type": "integer"
                },
                "extra_settings": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/ExtraSetting"
                    }
                }
            },
            "required": [
                "accelerator_code",
                "accelerator_desc",
                "batch_size",
                "extra_settings",
                "framework",
                "model_path"
            ],
            "title": "BackendSettings"
        },
        "ExtraSetting": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "value": {
                    "type": "string",
                    "format": "integer"
                },
                "value_name": {
                    "type": "string",
                    "format": "integer"
                }
            },
            "required": [
                "id",
                "name",
                "value",
                "value_name"
            ],
            "title": "ExtraSetting"
        }
