{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/ExtendedResultSchema",
    "definitions": {
        "ExtendedResultSchema": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "meta": {
                    "$ref": "#/definitions/Meta"
                },
                "results": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Result"
                    }
                },
                "environment_info": {
                    "$ref": "#/definitions/EnvironmentInfo"
                },
                "build_info": {
                    "$ref": "#/definitions/BuildInfo"
                }
            },
            "required": [
                "build_info",
                "environment_info",
                "meta",
                "results"
            ],
            "title": "ExtendedResultSchema"
        },
        "BuildInfo": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "version": {
                    "type": "string"
                },
                "build_number": {
                    "type": "string"
                },
                "build_date": {
                    "type": "string",
                    "format": "date-time"
                },
                "official_release_flag": {
                    "type": "boolean"
                },
                "dev_test_flag": {
                    "type": "boolean"
                },
                "backend_list": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "git_branch": {
                    "type": "string"
                },
                "git_commit": {
                    "type": "string"
                },
                "git_dirty_flag": {
                    "type": "boolean"
                }
            },
            "required": [
                "backend_list",
                "build_date",
                "build_number",
                "dev_test_flag",
                "git_branch",
                "git_commit",
                "git_dirty_flag",
                "official_release_flag",
                "version"
            ],
            "title": "BuildInfo"
        },
        "EnvironmentInfo": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "platform": {
                    "type": "string"
                },
                "value": {
                    "$ref": "#/definitions/Value"
                }
            },
            "required": [
                "platform",
                "value"
            ],
            "title": "EnvironmentInfo"
        },
        "Value": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "android": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/Android"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "ios": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/Ios"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "windows": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/Windows"
                        },
                        {
                            "type": "null"
                        }
                    ]
                }
            },
            "required": [
                "android",
                "ios",
                "windows"
            ],
            "title": "Value"
