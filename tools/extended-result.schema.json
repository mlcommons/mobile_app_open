                "loadgen_scenario": {
                    "type": "string"
                },
                "backend_settings": {
                    "$ref": "#/definitions/BackendSettings"
                },
                "performance_run": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/Run"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "accuracy_run": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/Run"
                        },
                        {
                            "type": "null"
                        }
                    ]
