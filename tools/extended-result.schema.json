            "title": "Result"
        },
        "Run": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "throughput": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/Throughput"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "accuracy": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/Accuracy"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "accuracy2": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/Accuracy"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "dataset": {
                    "$ref": "#/definitions/Dataset"
                },
                "measured_duration": {
                    "type": "number"
                },
                "measured_samples": {
                    "type": "integer"
                },
                "start_datetime": {
                    "type": "string"
                },
                "loadgen_info": {
                    "oneOf": [
                        {
                            "$ref": "#/definitions/LoadgenInfo"
                        },
                        {
                            "type": "null"
                        }
                    ]
                }
